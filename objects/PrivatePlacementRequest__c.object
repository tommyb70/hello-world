    <validationRules>
        <fullName>Consultant_Required_Fields</fullName>
        <active>true</active>
        <description>If the firm office is type of consultant, the user must provide either Represents Investors or Represents Individuals or both.</description>
        <errorConditionFormula>(FirmOffice__r.RecordType.DeveloperName = "Consultant") &amp;&amp;
NOT( RepresentsIndividuals__c ) &amp;&amp;
NOT( RepresentsInstitutions__c )</errorConditionFormula>
        <errorMessage>This Firm Office is categorized as a Consultant.  Please indicate whether the consultant represents individuals or investors or both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Firm_Types_Not_Allowed</fullName>
        <active>true</active>
        <description>Limits the firm types for which a private placement may be requested.</description>
        <errorConditionFormula>TEXT(FirmOffice__r.Type) &lt;&gt; "Home" &amp;&amp;
TEXT(FirmOffice__r.Type) &lt;&gt; "Branch" &amp;&amp;
TEXT(FirmOffice__r.Type) &lt;&gt; "Consultant"</errorConditionFormula>
        <errorMessage>You may only request private placement approval for Firm Types of Home, Branch, or Consultant.  Please change the Firm for which you are requesting approval or cancel this request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_Justification_Required</fullName>
        <active>true</active>
        <description>At least one Justification checkbox must be on</description>
        <errorConditionFormula>NOT( AccreditedInvestor__c ) &amp;&amp; NOT( QualifiedPurchaser__c ) &amp;&amp; NOT( SophisticatedInvestor__c )</errorConditionFormula>
        <errorMessage>You must choose at least one Justification</errorMessage>
    </validationRules>
</CustomObject>

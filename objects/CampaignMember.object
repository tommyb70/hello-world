    <validationRules>
        <fullName>Allow_Registration_Type_False</fullName>
        <active>true</active>
        <description>If the campaign has Allow Registration Types = FALSE, prohibit selection of Registration Type</description>
        <errorConditionFormula>NOT( Campaign.AllowRegistrationTypes__c ) &amp;&amp; NOT(ISBLANK(TEXT( RegistrationType__c )))</errorConditionFormula>
        <errorDisplayField>RegistrationType__c</errorDisplayField>
        <errorMessage>This campaign does not allow the selection of Registration Type.  Please set to none to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Add_Event_Mbr_If_Bounce</fullName>
        <active>true</active>
        <description>Event campaign members cannot be added if their email address has bounced.</description>
        <errorConditionFormula>IF(AND($Setup.NGAMSettingsHierarchy__c.ByPassEventCampMemberValidationRules__c == false, 
Campaign.Parent.CventCampaign__c == false), false , AND(RecordTypeOfCampaign__c = "Event", 
NOT(ISBLANK( Contact.EmailBouncedDate ))))</errorConditionFormula>
        <errorMessage>This contact/rep has had their email bounce.  Please contact the rep for a new email address before adding them to the event campaign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Add_Event_Mbr_If_Opt_Out</fullName>
        <active>true</active>
        <description>Event campaign members cannot be added if they have opted out of Pardot Emails unless user manually overrides using the Override checkbox.</description>
        <errorConditionFormula>If(AND($Setup.NGAMSettingsHierarchy__c.ByPassEventCampMemberValidationRules__c == false, 
Campaign.Parent.CventCampaign__c == false),false,
AND(RecordTypeOfCampaign__c = "Event" &amp;&amp;
Contact.HasOptedOutOfEmail &amp;&amp;
NOT( AdvisorConsent__c)))</errorConditionFormula>
        <errorDisplayField>AdvisorConsent__c</errorDisplayField>
        <errorMessage>This advisor has opted out of email communications.  By checking this box, you attest that you have received the advisor's consent to participate in communications related to this campaign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Add_Event_Mbr_if_No_Email</fullName>
        <active>true</active>
        <description>Event campaign members cannot be added if they don't have an email address.</description>
        <errorConditionFormula>If(OR($Setup.NGAMSettingsHierarchy__c.ByPassEventCampMemberValidationRules__c,NOT(Campaign.Parent.CventCampaign__c)),false,
AND(RecordTypeOfCampaign__c = "Event" &amp;&amp;
ISBLANK( Contact.Email )))</errorConditionFormula>
        <errorMessage>Please add an email address to the Contact/Rep before adding this campaign member.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Add_Event_Mbr_if_No_Pardot_Sync</fullName>
        <active>false</active>
        <description>If a contact is not syncing to Pardot, user cannot add them to Event Campaign.</description>
        <errorConditionFormula>RecordTypeOfCampaign__c = "Event" &amp;&amp;
ISBLANK( Contact.pi__url__c )</errorConditionFormula>
        <errorMessage>This Contact/Rep is not being synched to Pardot.  Please wait for 10 minutes and try to add the member again.  If the problem persists, submit an IT Service ticket before adding the rep to the event campaign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Event_Reg_Type</fullName>
        <active>false</active>
        <description>User cannot change the event registration type after an invitation has been sent from CVent</description>
        <errorConditionFormula>RecordTypeOfCampaign__c = "Event" &amp;&amp;  TEXT(Status) != "Added to List" &amp;&amp; ISCHANGED( RegistrationType__c )</errorConditionFormula>
        <errorDisplayField>RegistrationType__c</errorDisplayField>
        <errorMessage>You may not change the Registration Type after an invitation has been sent.  Please contact Event Planning to switch registration types.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Use_Pers_Text_in_Non_Event</fullName>
        <active>true</active>
        <errorConditionFormula>RecordTypeOfCampaign__c &lt;&gt; "Event" &amp;&amp;
ISCHANGED( InvitedByPersonalizationText__c )</errorConditionFormula>
        <errorMessage>You can only use Personalization Text with Event campaigns</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Registration_Type</fullName>
        <active>true</active>
        <description>If campaign has Enforce Registration Type = TRUE, ensure Registration Type is not null</description>
        <errorConditionFormula>Campaign.EnforceRegistrationTypes__c &amp;&amp; ISBLANK(TEXT( RegistrationType__c ))</errorConditionFormula>
        <errorDisplayField>RegistrationType__c</errorDisplayField>
        <errorMessage>This campaign requires a Registration Type to be selected.  Please navigate to the Contact/Rep, and add a row to "Campaign History".</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Member_Must_Have_Invited_By</fullName>
        <active>true</active>
        <description>Event campaign members must have an invited by</description>
        <errorConditionFormula>If(AND($Setup.NGAMSettingsHierarchy__c.ByPassEventCampMemberValidationRules__c == false, 
Campaign.Parent.CventCampaign__c == false),false, AND( RecordTypeOfCampaign__c = "Event" &amp;&amp;
ISBLANK(  InvitedBy__c ) &amp;&amp;
ISCHANGED (InvitedBy__c)))</errorConditionFormula>
        <errorDisplayField>InvitedBy__c</errorDisplayField>
        <errorMessage>All event campaign members must have an Invited By.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Member_Status_Add_Change</fullName>
        <active>true</active>
        <description>Non-system admins cannot change event member status or add a member in a status other than Added to List.</description>
        <errorConditionFormula>If(OR($Setup.NGAMSettingsHierarchy__c.ByPassEventCampMemberValidationRules__c,NOT(Campaign.Parent.CventCampaign__c)),false, AND( RecordTypeOfCampaign__c = "Event"
&amp;&amp; OR(ISCHANGED(Status),AND(ISNEW(),NOT(ISPICKVAL(Status,'Added to List'))))))</errorConditionFormula>
        <errorMessage>You cannot change an event campaign member status or add a member in a status other than Added to List.</errorMessage>
    </validationRules>
</CustomObject>

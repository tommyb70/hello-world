    <validationRules>
        <fullName>Cannot_Add_Update_Old_Tasks</fullName>
        <active>false</active>
        <description>Users cannot add or update old tasks older than the days in custom setting NGAM Settings - Hierarchy.Cannot CUD Activity After Days.</description>
        <errorConditionFormula>OR(AND(OR(TODAY() - PRIORVALUE( ActivityDate ) &gt;  $Setup.NGAMSettingsHierarchy__c.CannotCUDActivityAfterDays__c,TODAY() - ActivityDate &gt;  $Setup.NGAMSettingsHierarchy__c.CannotCUDActivityAfterDays__c),        
	  OR( ISCHANGED( ActivityComments__c ), 
          ISCHANGED( ActivityDate ), 
          ISCHANGED( WhoId ), 
          ISCHANGED( OwnerId ), 
          ISCHANGED( Type ), 
          ISCHANGED( ActivityType__c ), 
          ISCHANGED( ActivitySubType__c ), 
          ISCHANGED( Status ),
          ISCHANGED( WhatId ))),
      AND(ISNEW(),TODAY() - ActivityDate &gt; $Setup.NGAMSettingsHierarchy__c.CannotCUDActivityAfterDays__c)
)</errorConditionFormula>
        <errorMessage>This task is too old to add or update.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Seismic_Contact_Type_Change</fullName>
        <active>false</active>
        <description>Only 'Seismic LiveSend' and 'Email' Types and Activity Types can be changed to 'Seismic - Contact'.

INACTIVATING with LEX</description>
        <errorConditionFormula>AND( $Profile.Name != "System Administrator" &amp;&amp; $Profile.Name != "NGAM Integration",
OR (
AND (
	ISPICKVAL( ActivityType__c, "Seismic - Contact"), 
	NOT (
	OR(
		ISPICKVAL(PRIORVALUE( ActivityType__c) , "Seismic LiveSend"),
		ISPICKVAL(PRIORVALUE( ActivityType__c) , "Email")
	)
	)
),
AND (
	ISPICKVAL( Type, "Seismic - Contact"), 
	NOT (
	OR(
		ISPICKVAL(PRIORVALUE( Type) , "Seismic LiveSend"),
		ISPICKVAL(PRIORVALUE( Type) , "Email")
	)
	)
)
)
)</errorConditionFormula>
        <errorMessage>You can only change Seismic LiveSend and Email to Seismic - Contact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Users_Can_not_Select_Email</fullName>
        <active>false</active>
        <description>Users can not select the Activity Type &amp; Activity Sub Type = Email

INACTIVATING with LEX</description>
        <errorConditionFormula>( (ISCHANGED(ActivityType__c )  &amp;&amp;  ISPICKVAL( ActivityType__c , "Email"))  &amp;&amp; ( NOT(ISPICKVAL(PRIORVALUE( ActivityType__c), ""))))
|| (ISCHANGED( ActivitySubType__c  )  &amp;&amp;  ISPICKVAL( ActivitySubType__c , "Email")) &amp;&amp;  (  NOT (ISPICKVAL(PRIORVALUE( ActivitySubType__c), "")))</errorConditionFormula>
        <errorMessage>You can not select Email as an Activity Type or Activity Sub Type.  They are only created through the Outlook Integration</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Required_Field_Assigned_To</fullName>
        <active>true</active>
        <description>All tasks require the Assigned To field to be populated</description>
        <errorConditionFormula>ISBLANK(  OwnerId  )</errorConditionFormula>
        <errorMessage>All Activities require the Assigned To field to be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Required_Field_Comments</fullName>
        <active>true</active>

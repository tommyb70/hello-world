    <validationRules>
        <fullName>Sales_Manager_Campaign_Type_Update</fullName>
        <active>true</active>
        <description>Ensures that only sales managers can create a call campaign and assign a type value of 'Sales Manager Campaign'. The current user must have the 'VIP' field checked on the user object. This field is used to identify sales managers.</description>
        <errorConditionFormula>AND( 
$User.VIP__c  = FALSE, 
ISPICKVAL( Type , "Sales Manager Campaign" ))</errorConditionFormula>
        <errorDisplayField>Type</errorDisplayField>
        <errorMessage>Only sales managers can create a campaign with a 'Sales Manager Campaign' type. Please select the 'Personal Campaign' type.</errorMessage>
    </validationRules>

    <fields>
        <fullName>DocumentDate__c</fullName>
        <description>The effective date of the document.  For example, the Document Date would be 3/31 for a Q2 document even though the file record might have a created date of 4/15.</description>
        <externalId>false</externalId>
        <inlineHelpText>The effective date of the document.  For example, the Document Date would be 3/31 for a Q2 document even though the Financial Account File record might have a created date of 4/15.</inlineHelpText>
        <label>Document Date</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>DocumentQuarter__c</fullName>
        <description>Calculates the quarter of the document based on the Document Date</description>
        <externalId>false</externalId>
        <formula>CASE( MONTH( DocumentDate__c )  ,
1,"Q1",
2,"Q1",
3,"Q1",
4,"Q2",
5,"Q2",
6,"Q2",
7,"Q3",
8,"Q3",
9,"Q3",
10,"Q4",
11,"Q4",
12,"Q4",
"Q4")</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Calculates the quarter of the document based on the Document Date</inlineHelpText>
        <label>Document Quarter</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DocumentType__c</fullName>
        <description>The type of document/file that this record represents.</description>
        <externalId>false</externalId>
        <inlineHelpText>The type of document/file that this record represents</inlineHelpText>
        <label>Document Type</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Quarterly Performance Report</fullName>
                    <default>false</default>
                    <label>Quarterly Performance Report</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
